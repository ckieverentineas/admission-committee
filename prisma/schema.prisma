// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Passport {
  id       Int       @id @default(autoincrement())
  name  String
  firstname String
  lastname  String
  gender  String
  birthday  DateTime
  born  String
  pas_ser Int
  pas_num Int
  pas_get String
  pas_date  DateTime
  snils Int
  inn Int
}